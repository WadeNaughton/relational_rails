<%= form_with url: '/guests', method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :new %>
  <%= form.text_field :new %>
  <%= form.label :age %>
  <%= form.text_field :age %>
  <%= form.label :library_id %>
  <%= form.text_field :library_id %>
  <%= form.submit "Create Guest" %>
<% end %>
